<% @title=@event.name+" | Tech & Startup Events in Los Aneles" %>

<div class="container-fluid">
<p id="notice"><%= notice %></p>

<small><%= link_to 'Back to Home', events_path %></small>
<h4>
	<%= @event.starttime.to_date().strftime("%A %B %d, %Y") %>
	<% if @event.starttime.to_date() != @event.endtime.to_date() %>
		<%=	"to " + @event.endtime.to_date().strftime("%B %d, %Y") %>
	<% end %>
</h4>

<h1>
  <%= @event.name %>
</h1>

<p style="padding-bottom:20px">
  <%= link_to @event.website, @event.website %>
</p>
</div>
<iframe width="100%" height="500px" frameborder="0" src="<%= @event.website %>"></iframe>
